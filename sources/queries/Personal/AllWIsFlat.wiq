SELECT
    [System.Id],
    [System.WorkItemType],
    [System.Title],
    [System.State],
    [System.AreaPath],
    [Microsoft.VSTS.Scheduling.OriginalEstimate],
    [Microsoft.VSTS.Scheduling.CompletedWork],
    [Microsoft.VSTS.Scheduling.RemainingWork],
    [System.AssignedTo],
    [System.IterationPath],
    [System.Tags]
FROM workitems
WHERE
    [System.TeamProject] = '$projectName'

